<!--pages/Test/Test5/Test5.wxml-->
<view class="Container">
  <view class="TopBar">
    测试过程desu
  </view>
  <view class="Check">
    <image class="CheckIcon" src="Note.jpg"></image>
    评估依据
  </view>
  <view class="Number">
    <view class="NumberShow">
    5/11
    </view>
    是否有以下体感症状
  </view>
  <view class="Question">
    <view class="QuestionPart">
      <view class="QuestionContent">头冷头昏</view>
      <view class="QuestionChoose">
        <slider value="{{body1}}" bindchange="Cbody1" activeColor="#909090" class="QuestionSlider" max="2" step="1" block-size="15" ></slider>
        <view class="QuestionLabel">
          <view>无</view><view>轻</view><view>重</view>
        </view>
      </view>
    </view>
    <view class="QuestionPart">
      <view class="QuestionContent">体力下降</view>
      <view class="QuestionChoose">
        <slider value="{{body2}}" bindchange="Cbody2" activeColor="#909090" class="QuestionSlider" max="2" step="1" block-size="15" ></slider>
        <view class="QuestionLabel">
          <view>无</view><view>轻</view><view>重</view>
        </view>
      </view>
    </view>
    <view class="QuestionPart">
      <view class="QuestionContent">胸闷心慌</view>
      <view class="QuestionChoose">
        <slider value="{{body3}}" bindchange="Cbody3" activeColor="#909090" class="QuestionSlider" max="2" step="1" block-size="15" ></slider>
        <view class="QuestionLabel">
          <view>无</view><view>轻</view><view>重</view>
        </view>
      </view>
    </view>
    <view class="QuestionPart">
      <view class="QuestionContent">肌肉非运动酸痛</view>
      <view class="QuestionChoose">
        <slider value="{{body4}}" bindchange="Cbody4" activeColor="#909090" class="QuestionSlider" max="2" step="1" block-size="15" ></slider>
        <view class="QuestionLabel">
          <view>无</view><view>轻</view><view>重</view>
        </view>
      </view>
    </view>
    <view class="QuestionPart">
      <view class="QuestionContent">怕冷</view>
      <view class="QuestionChoose">
        <slider value="{{body5}}" bindchange="Cbody5" activeColor="#909090" class="QuestionSlider" max="2" step="1" block-size="15" ></slider>
        <view class="QuestionLabel">
          <view>无</view><view>轻</view><view>重</view>
        </view>
      </view>
    </view>
    <view class="QuestionPart">
      <view class="QuestionContent">非以上其他不适</view>
      <view class="QuestionChoose">
        <slider value="{{body6}}" bindchange="Cbody6" activeColor="#909090" class="QuestionSlider" max="2" step="1" block-size="15" ></slider>
        <view class="QuestionLabel">
          <view>无</view><view>轻</view><view>重</view>
        </view>
      </view>
    </view>
  </view>
  <view class="Answer">
   <view bindtap="Prev" class="AnswerPrev">上一项</view>
   <view bindtap="Next" class="AnswerNext">下一项</view>
  </view>
</view>

