          <view>无</view><view>轻</view><view>重</view>
        </view>
      </view>
    </view>
  </view>
  <view class="Answer">
   <view bindtap="Prev" class="AnswerPrev">上一题</view>
   <view bindtap="Next" class="AnswerNext">下一题</view>
  </view>
