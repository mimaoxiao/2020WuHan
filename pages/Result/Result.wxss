/* pages/Result/Result.wxss */
.Container{
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.Title{
  width: 100%;
  display: flex;
  flex-direction: column;
  background-color: #2373B0;
}
.TitleBig{
  font-size: 40rpx;
  padding: 5rpx 40rpx;
  color: white;
  padding-bottom: 15rpx;
}
.TitleSmall{
  padding: 5rpx 40rpx;
  padding-bottom: 20rpx;
  font-size: 30rpx;
  color: white;
}
.TitleImage{
  height: 100rpx;
}
.Analyse{
  flex-grow: 1;
  display: flex;
  margin-top: 20rpx;
  flex-direction: column;
  align-items: center;
}
.AnalyseStatus{
  font-size: 50rpx;
  font-weight: 500;
}
.AnalyseText{
  margin: 50rpx;
  font-size: 35rpx;
}
.Other{
  width: 100%;
  display: flex;
  justify-content: space-around;
  padding-bottom: 10rpx;
}
.OtherPart{
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 25rpx;
}
.OtherIcon{
  height: 75rpx;
  width: 75rpx;
}
.Human{
  border-top: 1px solid black;
  text-align: left;
  width: 100%;
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.HumanTip{
  text-align: left;
  color: red;
  padding: 50rpx;
  font-size: 25rpx;
}
.HumanSwitch{
  height: 80rpx;
  padding-top: 40rpx;
  text-align: center;
  background-color: #2783f2;
}