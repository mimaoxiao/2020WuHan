/* pages/Result/Result.wxss */
.Container{
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.Title{
  font-size: 75rpx;
  font-weight: bold;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.TitleText{
  height: auto;
}
.TitleImage{
  height: 100rpx;
}
.Analyse{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.AnalyseStatus{
  font-size: 50rpx;
  font-weight: bold;
}
.AnalyseText{
  margin: 50rpx;
}
.Other{
  width: 100%;
  display: flex;
  justify-content: space-around;
}
.OtherPart{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.OtherIcon{
  height: 50rpx;
  width: 50rpx;
}
.Human{
  border-top: 1px solid black;
  text-align: left;
  width: 100%;
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.HumanTip{
  text-align: left;
  width: 100%;
  padding: 50rpx;
}
.HumanSwitch{
  height: 80rpx;
  padding-top: 40rpx;
  text-align: center;
  background-color: #2783f2;
}